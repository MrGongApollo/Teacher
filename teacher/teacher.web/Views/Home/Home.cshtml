    .quick-actions-box>.quick-actions>li{height:8rem;margin:5px;margin-left:0;color:white;text-align:center;position:relative;cursor:pointer;}
    .quick-actions-box > .quick-actions > li:hover{background-color:#23363f!important;transition:all 0.3s ease;}
    .quick-actions-box > .quick-actions > li .label{position:absolute;top:-5px;right:-8px;z-index:1;}
    .quick-actions-box > .quick-actions > li .fa{width:100%;height:5rem;line-height:50px;text-align:center;font-size:5rem;display:block;padding:10px 0;color:white;margin-bottom:2px;}
    #data_chart{height:15rem;}
    .container-fluid>.row{margin-top:5px;}
</style>

